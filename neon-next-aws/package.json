{
  "name": "neon-next-aws",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "ada": "ada credentials update --account=339713184738 --provider=conduit --role=IibsAdminAccess-DO-NOT-DELETE --once",
    "dev": "npm run ada && sst bind next dev",
    "clean:next": "rm -rf .next",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deployStaging": "npm run ada && sst deploy --stage staging",
    "deploy": "npm run ada && sst deploy --stage production",
    "deploy:profile": "AWS_PROFILE=production sst deploy --stage production",
    "delete:prod": "npx sst remove --stage production",
    "delete:staging": "npx sst remove --stage staging"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.9.3",
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "aws-cdk-lib": "2.142.1",
    "constructs": "10.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "sst": "^2.42.0",
    "tailwindcss": "^3.4.1"
  }
}
